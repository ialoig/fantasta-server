{
  "name": "fantasta_server",
  "version": "1.0.0",
  "description": "Fantasta server module",
  "private": true,
  "scripts": {
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "pm2 start dist/index.js",
    "test_local": "nyc mocha test/**/*.test.js --exit",
    "test": "nyc mocha test/**/*.test.js --exit",
    "test-unit": "nyc mocha test/unit-test/*.test.js --exit",
    "test-integration": "nyc mocha test/integration/*.test.js --exit",
    "start": "npm run build && nodemon --exec babel-node ./fantasta.js",
    "debug": "npm run build && nodemon --exec babel-node --legacy-watch --watch src --inspect=0.0.0.0:9222 --nolazy ./fantasta.js",
    "docker-debug": "docker-compose -f ./docker-compose.yml -f ./docker-compose.debug.yml up --build --remove-orphans",
    "docker-test": "docker-compose -f ./docker-compose.yml -f ./docker-compose.test.yml up -d --build  --remove-orphans && docker exec -it fantasta_server npm run test ; docker-compose down",
    "docker-test-integration": "docker-compose -f ./docker-compose.yml -f ./docker-compose.test.yml up -d --build --remove-orphans && docker exec -it fantasta_server npm run test-integration ; docker-compose down",
    "docker-prod": "docker-compose -f ./docker-compose.yml -f ./docker-compose.prod.yml up --build",
    "docker-build": "docker-compose build"
  },
  "dependencies": {
    "config": "^3.3.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "8.5.1",
    "i18n-js": "^3.8.0",
    "lodash": "4.17.20",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "node-mandrill": "^1.0.1",
    "node-schedule": "1.3.2",
    "pm2": "^4.5.0",
    "prom-client": "^13.1.0",
    "pug": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "3.0.4",
    "validator": "^13.5.2",
    "xlsx": "0.16.7"
  },
  "devDependencies": {
    "debug": "4.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://pinkal2019@dev.azure.com/pinkal2019/FantaAsta/_git/Fantasta_server",
    "ssh": "git@ssh.dev.azure.com:v3/pinkal2019/FantaAsta/Fantasta_server"
  },
  "keywords": [
    "fantasta",
    "server",
    "service"
  ],
  "author": "pinkal",
  "license": "ISC"
}
